{
    "manifest_version": 3,
    "name": "TP UX for IPSC Ontario",
    "version": "1.0",
    "icons": {
        "16": "icon.png",
        "32": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://reg.ipsc-ont.org/*"
            ],
            "js": [
                "lib/jquery.min.js",
                "lib/bootstrap/js/bootstrap.bundle.min.js",
                "lib/DataTables/js/jquery.dataTables.min.js",
                "lib/bootstrapTable/bootstrap-table.min.js",
                "utils/matchTable.js",
                "content.js"
            ],
            "css": [
                "lib/DataTables/css/jquery.dataTables.min.css",
                "lib/bootstrapTable/bootstrap-table.min.css"
            ]
        }
    ]
}
