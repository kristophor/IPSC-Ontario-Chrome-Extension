{
    "manifest_version": 3,
    "name": "IPSC Ontario Better UX Extension",
    "version": "1.0",
    "icons": {
        "16": "icon.png",
        "32": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
        },
        "browser_action": {
          "default_icon": {
            "16": "icon16.png",
            "32": "icon32.png",
            "48": "icon48.png",
            "128": "icon128.png"
          }
        },
    "content_scripts": [
        {
            "matches": [
                "https://reg.ipsc-ont.org/*"
            ],
            "js": [
                "lib/jquery.min.js",
                "lib/bootstrap/js/bootstrap.bundle.min.js",
                "lib/DataTables/js/jquery.dataTables.min.js",
                "lib/bootstrapTable/bootstrap-table.min.js",
                "utils/matchTable.js",
                "content.js"
            ],
            "css": [
                "lib/DataTables/js/jquery.dataTables.min.js",
                "lib/bootstrapTable/bootstrap-table.min.css"
            ]
        }
    ]
}